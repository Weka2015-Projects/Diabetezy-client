{
  "name": "Diabetezy-client",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "start": "budo ./index.jsx -t babelify -d --serve bundle.js --live",
    "build": "browserify ./index.jsx -t babelify -o bundle.js -g uglifyify",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm run test -- --watch",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelmcgrath/Diabetezy-client"
  },
  "author": "michaelmcgrath",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michaelmcgrath/Diabetezy-client/issues"
  },
  "homepage": "https://github.com/michaelmcgrath/Diabetezy-client",
  "dependencies": {
    "chart.js": "^1.0.2",
    "firebase": "^2.4.0",
    "immutable": "^3.7.6",
    "jquery": "^2.2.0",
    "moment": "^2.11.2",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-chart.js": "^1.0.0",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "react-router-bootstrap": "^0.20.1",
    "superagent": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "gh-pages": "^0.3.0",
    "history": "^2.0.0",
    "immutable": "^3.7.6",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "redux": "^3.3.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  }
}
