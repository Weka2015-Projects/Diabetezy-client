{
  "name": "Diabetezy-client",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test.js | smokestack | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": ["babelify"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelmcgrath/Diabetezy-client"
  },
  "author": "michaelmcgrath",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michaelmcgrath/Diabetezy-client/issues"
  },
  "homepage": "https://github.com/michaelmcgrath/Diabetezy-client",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "gh-pages": "^0.3.0",
    "react": "^0.14.7", 
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  }
}
